@model IEnumerable<CrossoverSpa.ViewModel.ControllerActionsForRoutingViewModel>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Your accessible features. <span class="caret"></span></a>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
            @*<a asp-controller=@item.ControllerName asp-action=@item.ActionName>@item.DisplayName</a>*@
            <li><a asp-controller=@item.ControllerName asp-action=@item.ActionName>@item.DisplayName</a></li>
        }
    </ul>
</li>